{ 
        "Interface":{
          "title":"Interface",
          "data":[
          {
            "id":"0",
            "title":"show interface",
            "command":"[admin@MikroTik] >interface print"
          },
          {
            "id":"1",
            "title":"disable ether",
            "command":"[admin@MikroTik] >interface ethernet disable etherx"
          },
          {
            "id":"2",
            "title":"enable ether",
            "command":"[admin@MikroTik] >interface ethernet enable etherx"
          },       
          {
            "id":"3",
            "title":"monitor traffic",
            "command":"[admin@MikroTik] >interface monitor-traffic etherx"
          },
          {
            "id":"4",
            "title":"add comment to ether",
            "command":"[admin@MikroTik] >interface ethernet set comment=INTERNET numbers=etherx"
          },       {
            "id":"5",
            "title":" refer to number card 0",
            "command":"[admin@MikroTik] >interface ethernet monitor 0"
          }
        ]
      },
      "SetupVLAN":{
        "title":"Setup VLAN",
        "data":[
        {
          "id":"0",
          "command":"[admin@MikroTik] >interface vlan add name=vlan10 vlan_id4 interface =etherx"
        },
        {
          "id":"1",
          "command":"[admin@MikroTik] >interface vlan print"
        },
        {
          "id":"2",
          "command":"[admin@MikroTik] >interface vlan disable 0"
        }
      ]
    },
    "System":{
