      },
      {
        "id":"4",
        "title":"Change router name",
        "command":"[admin@MikroTik] > system identity set name=MK"
      },
      {
        "id":"5",
        "title":"license level",
        "command":"[admin@MikroTika] > system license print"
      },
      {
        "id":"6",
